<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:MyNotes.Themes">

    <!--General-->
    <SolidColorBrush x:Key="NormalBorderBrush" Color="#FFCCCCCC"></SolidColorBrush>
    <SolidColorBrush x:Key="NormalBackgroundBrush" Color="White"></SolidColorBrush>
    <SolidColorBrush x:Key="MouseOverBackgroundBrush" Color="#FFF6F6F6"></SolidColorBrush>
    <SolidColorBrush x:Key="SelectedBackgroundBrush" Color="#FFEEEEEE"></SolidColorBrush>

    <SolidColorBrush x:Key="FillIconBrush" Color="#FF404040"></SolidColorBrush>
    <SolidColorBrush x:Key="IsEnabledForegroundBrush" Color="#FFCCCCCC"></SolidColorBrush>

    <!--TextBox-->
    <SolidColorBrush x:Key="IsFocusedBorderBrush" Color="#FFA4A4A4"></SolidColorBrush>

    
    
</ResourceDictionary>